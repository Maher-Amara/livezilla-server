<div class="TCBB">
	<table cellspacing="0" class="TCB header_class_placeholder">
        <tr>
            <td style="width: 50px; white-space: nowrap; vertical-align: top;"><!--group_label-->&nbsp;&nbsp;</td>
            <td class="last"><b><!--group_name--></b> (<!--receivers-->)</td>
        </tr>
		<tr>
			<td style="width: 50px; white-space: nowrap; vertical-align: top;"><!--name_label-->&nbsp;&nbsp;</td>
			<td class="last"><!--user--></td>
		</tr>
		<tr>
			<td style="white-space: nowrap; vertical-align: top;"><!--email_label-->&nbsp;&nbsp;</td>
			<td><a class="lz_chat_mail" href="#" data-url="mailto:<!--email-->" onclick="openLink(\'mailto:<!--email-->\');"><!--email--></a></td>
		</tr>
		<tr>
			<td style="white-space: nowrap; vertical-align: top;"><!--company_label-->&nbsp;&nbsp;</td>
			<td><!--company--></td>
		</tr>
		<tr>
			<td style="white-space: nowrap; vertical-align: top;"><!--phone_label-->&nbsp;&nbsp;</td>
			<td><!--phone--></td>
		</tr>
        <tr>
            <td style="white-space: nowrap; vertical-align: top;"><!--chat_id_label-->&nbsp;&nbsp;</td>
            <td><!--chat_id--></td>
        </tr>
        <tr>
            <td style="white-space: nowrap; vertical-align: top;"><!--area_code_label-->&nbsp;&nbsp;</td>
            <td><!--area_code--></td>
        </tr>
        <tr>
            <td style="white-space: nowrap; vertical-align: top;"><!--url_label-->&nbsp;&nbsp;</td>
            <td><a class="lz_chat_link_no_icon" href="#" data-url="<!--url-->" onclick="openLink(\'<!--url-->\');"><!--url--></a></td>
        </tr>
		<!--custom_fields-->
	</table>
</div>
